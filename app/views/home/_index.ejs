<div class="listProducts" data-image-id="{{image_id}}">
    {{#each results}}
    <div class="item product">
            <div class="picture">
                <a href="#" class="image">
                    <img src="{{image_url}}" alt=""> <br>
                </a>
            </div>
        </a>
    </div>
    {{/each}}
</div>
<span class="next-page" data-next-page="{{next_page}}" data-total="{{total}}"></span>



